{
  "name": "ts-setup",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r module-alias/register src/main.ts",
    "start:prod": "node -r module-alias/register dist/main.js",
    "test": "ava",
    "coverage": "nyc ava",
    "lint": "tslint --format stylish src/**/*.ts",
    "clean": "rm -rf dist/*",
    "build": "tsc -p tsconfig.json",
    "prebuild": "yarn clean",
    "build:prod": "tsc -p tsconfig.prod.json",
    "prebuild:prod": "yarn clean",
    "build:debug": "tsc -p tsconfig.json",
    "sandbox": "ts-node ./sandbox.ts",
    "postinstall": "yarn build"
  },
  "keywords": [],
  "author": "Arpple",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.13",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^10.11.3",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "ava": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^5.0.0",
    "typescript": "^3.4.5"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "cache": false,
    "sourceMap": true
  },
  "_moduleAliases": {
    "~": "dist"
  }
}
